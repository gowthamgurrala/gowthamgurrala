<?xml version="1.0" encoding="UTF-8"?>
<beans
xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<bean id="a1" class="com.gowtham.demo.Address">
<constructor-arg value="6-100"></constructor-arg>
<constructor-arg value="hyderabad"></constructor-arg>
<constructor-arg>
<ref bean="a2"/>
</constructor-arg>
</bean>
<bean id="a2" class="com.gowtham.demo.Company">
<constructor-arg value="ojas"></constructor-arg>
</bean>
<bean id="e" class="com.gowtham.demo.Employee">
<constructor-arg value="10" type="int"></constructor-arg>
<constructor-arg value="gowtham"></constructor-arg>
<constructor-arg value="10000.00"></constructor-arg>
<constructor-arg>
<ref bean="a1"/>
</constructor-arg>
</bean>

</beans>